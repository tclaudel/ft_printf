%nsigned int	unb[18] = {0, %INT_MA%, ft_random_unb(1), ft_random_unb(1), ft_random_unb(2), ft_random_unb(3), ft_random_unb(4), ft_random_unb(5), ft_random_unb(6), ft_random_unb(7), ft_random_unb(8), ft_random_unb(9), ft_random_unb(10), ft_random_unb(11), ft_random_unb(12), ft_random_unb(13), ft_random_unb(14), ft_random_unb(15)};

	i = 0;
	while (i < 20)
	{
		tested++;
		print_testing("\"|%%|\\n\",  random");
		if (printf("printf    :\t|%%|\n") == ft_printf("ft_printf :\t|%%|\n"))
			print_ok(&testok);
		else
			print_error(&testko);
		i++;
	}

